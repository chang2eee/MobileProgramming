<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamProject">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".YutGame" android:label="Yut 액티비티"/>
        <activity android:name=".SpinningBottleGame" android:label="Spinning Bottle 액티비티"/>
        <activity android:name=".RouletteGame" android:label="Spinning Roulette 액티비티"/>
        <activity android:name=".DiceGame" android:label="Roll Dice 액티비티"/>
        <activity android:name=".FlipCoinGame" android:label="Flip Coin 액티비티"/>
        <activity android:name=".RockPaperScissorsGame" android:label="Rock Paper Scissors 액티비티"/>
    </application>

</manifest>